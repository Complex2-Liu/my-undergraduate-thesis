\newtheorem{cor}[thm]{\namecor}
\newtheorem{lem}[thm]{\namelem}
\newtheorem{prop}[thm]{\nameprop}
\newenvironment{claim}[1][断言]
{\par\medskip\noindent\bbf{#1.} }
{\par\medskip}
